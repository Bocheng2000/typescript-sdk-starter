    "starter",
    "webpack",
    "editorconfig",
    "prettier",
    "eslint",
    "husky",
    "lint-staged",
    "standard-version",
    "typedoc",
    "jest"
  ],
  "author": "菜猫子neko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nekobc1998923/typescript-sdk-starter/issues"
  },
  "homepage": "https://github.com/nekobc1998923/typescript-sdk-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
